{
  "name": "laravel-casts-maker",
	"displayName": "Laravel Casts Maker",
	"description": "Fill casts automaticly with attributes list",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:laravel-casts-maker.fixCasts"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "laravel-casts-maker.fixCasts",
            "title": "Fix Casts"
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.64.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "eslint": "^8.9.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.1",
        "typescript": "^4.5.5",
        "@vscode/test-electron": "^2.1.2"
    }

}
